stream
  |from()
    .database('test')
    .measurement('iot')
    .where(lambda: "type" == 'temp')
  |window()
    .period(1m)
    .every(1m)
  |mean('value')
    .as('mean_value')
  |alert()
    .log('/home/wisniak199/kapacitor.log')
    .info(lambda: "mean_value" > 23)
    .exec('/home/wisniak199/iot/serwer/scripts/temp.py')
    .warn(lambda: "mean_value" < 19)
